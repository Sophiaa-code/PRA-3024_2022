        "c = ROOT.TCanvas(\"XXX\", \"X\")\n",
        "\n",
        "\n",
        "#extract the total P for all daughter particles\n",
        "tree.Draw(\"H1_Ptot\"+\">>h1(1000,0,300000)\")\n",
        "tree.Draw(\"H2_Ptot\"+\">>h2(1000,0,300000)\")\n",
